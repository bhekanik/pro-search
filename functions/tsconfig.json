{
	"compilerOptions": {
		"module": "commonjs",
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"importHelpers": true,
		"forceConsistentCasingInFileNames": true,
		"outDir": "lib",
		"sourceMap": true,
		"strict": true,
		"target": "es2017"
	},
	"compileOnSave": true,
	"include": ["src"]
}
