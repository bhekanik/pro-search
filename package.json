{
	"name": "pro-search",
	"version": "0.0.1",
	"author": "Bhekani Khumalo",
	"license": "MIT",
	"scripts": {
		"dev": "vite dev",
		"dev:https": "dev --https",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --write --plugin-search-dir=. . && eslint --ignore-path .gitignore ./src/",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"cy:open-ct": "cypress open-ct",
		"cy:run-ct": "cypress run-ct",
		"prepare": "husky install"
	},
	"dependencies": {
		"@sentry/browser": "^7.64.0",
		"@sentry/tracing": "^7.64.0",
		"@splitsoftware/splitio": "^10.23.0",
		"@supabase/supabase-js": "^2.33.1",
		"autoprefixer": "^10.4.15",
		"case": "^1.6.3",
		"daisyui": "^3.6.4",
		"fast-fuzzy": "^1.12.0",
		"logrocket": "^5.0.1",
		"qrcode": "^1.5.3",
		"supabase-ui-svelte": "^0.1.1",
		"theme-change": "^2.5.0"
	},
	"devDependencies": {
		"@cypress/vite-dev-server": "^5.0.5",
		"@sveltejs/adapter-auto": "1.0.0-next.65",
		"@sveltejs/adapter-vercel": "^1.0.0-next.87",
		"@sveltejs/kit": "^1.0.0-next.449",
		"@sveltejs/vite-plugin-svelte": "^1.4.0",
		"@tailwindcss/typography": "^0.5.9",
		"@testing-library/cypress": "^9.0.0",
		"@types/node": "^20.5.7",
		"@types/qrcode": "^1.5.1",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"autoprefixer": "^10.4.15",
		"cypress": "^12.17.4",
		"cypress-svelte-unit-test": "^3.3.4",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"husky": "^8.0.3",
		"lint-staged": "^14.0.1",
		"postcss": "^8.4.28",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^3.59.2",
		"svelte-check": "^3.5.0",
		"svelte-feather-icons": "^4.0.1",
		"svelte-preprocess": "^5.0.4",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^3.2.7"
	},
	"type": "module",
	"lint-staged": {
		"src/**/*.{js,ts,svelte}": "npm run lint",
		"src/**/*.{ts,svelte}": "npm run check",
		"functions/**/*.{js,ts}": "npm run lint",
		"src/**/*.{js,css,md,svelte,ts}": "npm run format"
	}
}
