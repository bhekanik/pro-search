{
	"name": "pro-search",
	"version": "0.0.1",
	"license": "MIT",
	"scripts": {
		"dev": "svelte-kit dev --host --open --port 4000",
		"dev:https": "svelte-kit dev --host --open --https",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --write --plugin-search-dir=. . && eslint --ignore-path .gitignore ./src/",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"cy:open-ct": "cypress open-ct",
		"cy:run-ct": "cypress run-ct",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@cypress/vite-dev-server": "^2.2.2",
		"@sveltejs/adapter-vercel": "^1.0.0-next.30",
		"@sveltejs/kit": "next",
		"@sveltejs/vite-plugin-svelte": "^1.0.0-next.32",
		"@tailwindcss/typography": "^0.5.0",
		"@testing-library/cypress": "^8.0.2",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"autoprefixer": "^10.4.0",
		"cypress": "^9.2.0",
		"cypress-svelte-unit-test": "^3.3.4",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"husky": "^7.0.0",
		"lint-staged": "^12.1.4",
		"postcss": "^8.4.5",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"svelte": "^3.42.6",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.9.8",
		"tailwindcss": "^3.0.8",
		"tslib": "^2.3.1",
		"typescript": "^4.5.4"
	},
	"type": "module",
	"dependencies": {
		"@auth0/auth0-spa-js": "^1.19.3",
		"@netlify/functions": "^0.7.2",
		"@sentry/browser": "^6.16.1",
		"@sentry/tracing": "^6.16.1",
		"@splitsoftware/splitio": "^10.16.1",
		"@types/node": "^14.17.20",
		"case": "^1.6.3",
		"daisyui": "^1.16.2",
		"fast-fuzzy": "^1.11.1",
		"firebase": "^9.6.6",
		"logrocket": "^2.1.3",
		"qrcode": "^1.5.0",
		"theme-change": "^2.0.2",
		"uuid": "^8.3.2"
	},
	"lint-staged": {
		"src/**/*.{js,ts,svelte}": "npm run lint",
		"src/functions/**/*.{js,ts}": "npm run lint",
		"src/**/*.{js,css,md,svelte,ts}": "npm run format"
	}
}
